{
  "expo": {
    "name": "expo-sdk-payment-example",
    "slug": "expo-sdk-payment-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tmejane.exposdkpaymentexample"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tmejane.exposdkpaymentexample"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [["./plugins/withLyra"], ["./plugins/withLyraCardRecognizer"]]
  }
}
